{
  "openapi": "3.0.1",
  "info": {
    "title": "BizRules API",
    "version": "v1"
  },
  "paths": {
    "/api/Calculation/list": {
      "post": {
        "tags": ["Calculation"],
        "requestBody": {
          "content": {
            "application/json-patch+json": {
              "schema": {
                "$ref": "#/components/schemas/CalculationFilter"
              }
            },
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CalculationFilter"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CalculationFilter"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CalculationFilter"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CalculationListItemDto"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CalculationListItemDto"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CalculationListItemDto"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CalculationFilter": {
        "type": "object",
        "properties": {
          "top": {
            "type": "integer",
            "format": "int32"
          },
          "searchQuery": {
            "type": "string",
            "nullable": true
          },
          "calculationTypeCodes": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CalculationListItemDto": {
        "type": "object",
        "properties": {
          "calculationID": {
            "type": "integer",
            "format": "int32"
          },
          "calculationTypeCode": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "isActive": {
            "type": "boolean"
          },
          "enterpriseID": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "CalculationVersionListItemDto": {
        "type": "object",
        "properties": {
          "calculationVersionID": {
            "type": "integer",
            "format": "int32"
          },
          "isDeleted": {
            "type": "boolean"
          },
          "calculationID": {
            "type": "integer",
            "format": "int32"
          },
          "calculationStatusCode": {
            "type": "string",
            "nullable": true
          },
          "revisionFrom": {
            "type": "string",
            "format": "date-time"
          },
          "revisionTo": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": {
          "type": "object",
          "additionalProperties": false
        }
